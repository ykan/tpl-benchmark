doctype html
html
  head
    meta(charset="utf-8")
    meta(content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport")
    title #{title}
    link(rel="stylesheet" href=cssURL)
  body
    div
      ul
        - for (var i = 0, l = names.length; i < l; i ++)
          - if (i % 2)
            li 1:#{names[i].name}
          - else
            li 2:#{names[i].name}

    script(src=jsURL)
